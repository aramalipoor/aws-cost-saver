{
  "name": "aws-cost-saver",
  "description": "A tiny CLI tool to help save costs in development environments when you're asleep and don't need them!",
  "version": "0.0.4",
  "author": "aramalipoor",
  "bin": {
    "aws-cost-saver": "./bin/run"
  },
  "bugs": "https://github.com/aramalipoor/aws-cost-saver/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "tslib": "^1.13.0",
    "aws-sdk": "^2.730.0",
    "chalk": "^4.1.0",
    "cli-ux": "^5.4.10",
    "inquirer": "^7.3.3",
    "listr": "^0.14.3",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.28",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "@types/aws-sdk": "^2.7.0",
    "@types/inquirer": "^7.3.1",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.159",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/aramalipoor/aws-cost-saver",
  "keywords": [
    "aws",
    "cost",
    "cost-saving"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "aws-cost-saver",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "aramalipoor/aws-cost-saver",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
